from luban.applications.UIApp import Extension as base

class Ext(base):
    
    def _init(self):
        super(Ext, self)._init()
        director = self.director
        orm = director.clerk.orm
        from mcvineui.dom import mapAllTypes
        mapAllTypes(orm)
        return
    

def extension():
    return Ext('mcvine-uiapp-ext')
